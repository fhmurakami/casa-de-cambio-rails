<%= form_for @user, :url => {:action => :new} do |f| %>
  <%= f.label :name, 'Nome' %>
  <%= f.text_field :name %>
  <%= f.label :email, 'E-mail' %>
  <%= f.text_field :email %>
  <%= f.label :cpf, 'CPF' %>
  <%= f.text_field :cpf %>
  
  <%= f.submit 'Cadastrar', class: "button btn_submit" %>
  <%= link_to 'Cancelar', root_path, class: "button btn_cancel" %>
<% end %>